  "description": "A component library written in intact for intact and vue",
  "browser": "@css/components/index.js",
  "main": "dist/kpc.js",
  "module": "components/index.js",
  "scripts": {
    "test": "karma start test/karma.conf.js",
    "snapshot": "UPDATE=1 npm run test",
    "cover": "UPDATE=2 npm run test",
    "start": "node server.js",
    "build": "NODE_ENV=production gulp build",
    "dev:doc": "gulp watch",
    "build:doc": "NODE_ENV=production gulp build:doc",
    "deploy:doc": "NODE_ENV=production gulp deploy:doc",
    "prepublishOnly": "npm run build && git commit -am 'chore: build' && git push",
    "release": "npm run release-patch",
    "prelease": "npm version prerelease && git push && npm publish",
    "release-patch": "git checkout master && npm version patch && git push && npm publish",
    "release-minor": "git checkout master && npm version minor && git push && npm publish",
    "release-major": "git checkout master && npm version major && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksc-fe/kpc.git"
  },
  "keywords": [
    "component",
    "intact",
    "vue"
  ],
  "files": [
    "components",
    "@css",
    "@stylus",
    "styles",
    "dist"
  ],
