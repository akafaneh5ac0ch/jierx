const Button = props => {
    let tagName = props.tagName;
    if (!tagName && props.href) {
        tagName = 'a';
    }
    return h(tagName || 'button', props, props.children);
};

const className = `k-btn k-${self.get('type')} ${self.get('className') || ''}` +
    ` ${self.get('disabled') ? 'k-disabled' : ''}`;

<Button 
    class={{ className }}
    {{ ...self.get() }}
/>
