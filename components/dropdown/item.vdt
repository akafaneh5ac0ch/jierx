/* import {proxyEvent} from '../utils'; */

const {children, disabled, className, _isFocus} = self.get();

const classNameObj = {
    'k-item': true,
    'k-disabled': disabled,
    [className]: className,
    'k-hover': _isFocus,
}

<div class={{ classNameObj }}
    ev-click={{ self._onClick.bind(self) }}
    ev-mouseenter={{ self._onMouseEnter.bind(self) }}
    ev-mouseleave={{ self._onMouseLeave.bind(self) }}
>{{ children }}</div>
