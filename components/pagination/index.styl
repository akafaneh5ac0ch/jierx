@require('./variables.styl');

.k-pagination
  li
    border 1px #d9d9d9 solid
    display inline
    padding 5px 15px
    cursor pointer
  a
    color #333
  .disabled
    cursor not-allowed
    a
      cursor not-allowed
  .k-pagination-active
    color #fff
    background-color #595959
    a
      color: #fff