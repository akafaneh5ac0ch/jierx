import Table from 'kpc/components/table';
var scheme = {a: '表头'};
var data1 = [{a: '多选框'}];
var data2 = [{a: '单选框'}];
var data3 = [{a: '无选框'}];

<div class='no-data-template'>
    <Table scheme={{scheme}} data={{data1}} />
    <Table scheme={{scheme}} data={{data2}} checkType='radio'/>
    <Table scheme={{scheme}} data={{data3}} checkType='none'/>
</div>