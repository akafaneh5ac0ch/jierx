    "contents": "<h1 id='header-%E7%A4%BA%E4%BE%8B'>示例</h1><h1 id='header-%E5%B1%9E%E6%80%A7'>属性</h1><div class=\"k-table-wrapper k-border\">\n                    <table class=\"k-table\">\n                        <thead><tr>\n<th>属性</th>\n<th>说明</th>\n<th>类型</th>\n<th>默认值</th>\n</tr>\n</thead>\n                        <tbody><tr>\n<td>title</td>\n<td>弹窗标题</td>\n<td><code>String</code></td>\n<td><code>&quot;提示&quot;</code></td>\n</tr>\n<tr>\n<td>value</td>\n<td>弹窗是否展示出来</td>\n<td><code>Boolean</code></td>\n<td><code>false</code></td>\n</tr>\n<tr>\n<td>size</td>\n<td>弹窗尺寸</td>\n<td><code>&quot;default&quot;</code> &#124; <code>&quot;small&quot;</code></td>\n<td><code>&quot;default&quot;</code></td>\n</tr>\n</tbody>\n                    </table>\n                </div><h1 id='header-%E6%96%B9%E6%B3%95'>方法</h1><div class=\"k-table-wrapper k-border\">\n                    <table class=\"k-table\">\n                        <thead><tr>\n<th>方法名</th>\n<th>说明</th>\n<th>参数</th>\n<th>返回值</th>\n</tr>\n</thead>\n                        <tbody><tr>\n<td>show</td>\n<td>弹出弹窗</td>\n<td>-</td>\n<td><code>undefined</code></td>\n</tr>\n<tr>\n<td>close</td>\n<td>关闭弹窗</td>\n<td>-</td>\n<td><code>undefined</code></td>\n</tr>\n</tbody>\n                    </table>\n                </div><h1 id='header-%E4%BA%8B%E4%BB%B6'>事件</h1><div class=\"k-table-wrapper k-border\">\n                    <table class=\"k-table\">\n                        <thead><tr>\n<th>事件名</th>\n<th>说明</th>\n<th>参数</th>\n</tr>\n</thead>\n                        <tbody><tr>\n<td>cancel</td>\n<td>点击取消按钮时触发</td>\n<td>-</td>\n</tr>\n<tr>\n<td>ok</td>\n<td>点击确定按钮时触发</td>\n<td>-</td>\n</tr>\n</tbody>\n                    </table>\n                </div>",
