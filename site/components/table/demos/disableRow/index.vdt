import Table from 'kpc/components/table';

var scheme = {a: '奇数行不可点击'};
var data = [{a: '1'}, {a: '2'}, {a: '3'}, {a: '4'}, {a: '5'}]


<div class='no-data-template'>
    <Table scheme={{ scheme }} data={{ data }} disableRow={{ function(data, index) {
        if (index%2 != 0) {
            return true;
        }
    }}}/>
</div>